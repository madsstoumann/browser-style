  "rateCategories": {
    "rate1": {
      "description": "Hastighed under 100 km/t, personbil/motorcykel uden anhænger",
      "conditions": {
        "speedLimit": "< 100",
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate2": {
      "description": "Hastighed 100 km/t eller over, personbil/motorcykel",
      "conditions": {
        "speedLimit": ">= 100", 
        "vehicle": ["car", "mc"],
        "trailer": false
      }
    },
    "rate3": {
      "description": "Bus, lastbil eller køretøj med anhænger",
      "conditions": {
        "vehicle": ["bus", "truck", "bus100"],
        "trailer": true
      }
    }
  },
  "consequenceTypes": {
    "UNCONDITIONAL_LICENSE_LOSS": {
      "id": "UNCONDITIONAL_LICENSE_LOSS",
      "severity": 5,
      "messageKey": "ubetinget_frakendelse",
      "preventsFine": true
    },
    "CONDITIONAL_LICENSE_LOSS": {
      "id": "CONDITIONAL_LICENSE_LOSS", 
      "severity": 4,
