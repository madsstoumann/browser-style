{
  "fontSize": {
    "$type": "dimension",
    "fluid": {
      "responsive": {
        "$value": {
          "value": 16,
          "unit": "px"
        },
        "$description": "Fluid font size using clamp",
        "$extensions": {
          "com.example.fluid": {
            "css": "clamp(1rem, 0.5rem + 2vw, 3rem)",
            "min": {
              "value": 16,
              "unit": "px"
            },
            "preferred": "0.5rem + 2vw",
            "max": {
              "value": 48,
              "unit": "px"
            }
          }
        }
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "containerPadding": {
      "$value": {
        "value": 24,
        "unit": "px"
      },
      "$description": "Responsive container padding",
      "$extensions": {
        "com.example.fluid": {
          "css": "max(24px, 5vw)",
          "function": "max",
          "values": [
            {"value": 24, "unit": "px"},
            "5vw"
          ]
        }
      }
    }
  }
}